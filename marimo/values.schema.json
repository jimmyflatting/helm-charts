{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "onyxia": {
        "type": "object",
        "title": "Onyxia Integration",
        "properties": {
            "s3": {
                "type": "object",
                "title": "S3 Integration",
                "properties": {
                    "enabled": {
                        "type": "boolean",
                        "title": "Enable S3 Integration",
                        "default": true,
                        "form": true
                    }
                }
            },
            "auth": {
                "type": "object",
                "title": "Authentication",
                "properties": {
                    "enabled": {
                        "type": "boolean",
                        "title": "Enable OIDC Authentication",
                        "default": true,
                        "form": true
                    }
                }
            }
        }
    }
}